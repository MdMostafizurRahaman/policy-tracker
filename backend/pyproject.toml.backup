[tool.poetry]
name = "policy-tracker-backend"
version = "1.0.0"
description = "Policy Tracker Backend API"
authors = ["Policy Tracker Team"]
readme = "README.md"

[tool.poetry.dependencies]
python = "^3.11"
fastapi = "^0.104.0"
uvicorn = {extras = ["standard"], version = "^0.24.0"}
pydantic = "^2.5.0"
pydantic-settings = "^2.1.0"
python-dotenv = "^1.0.0"
python-multipart = "^0.0.6"
bcrypt = "^4.1.0"
PyJWT = "^2.8.0"
email-validator = "^2.1.0"
requests = "^2.31.0"
httpx = "^0.25.0"
boto3 = "^1.34.0"
redis = "^5.0.0"
motor = "^3.3.0"
numpy = "^1.24.0"
pandas = "^2.0.0"
scipy = "^1.11.0"
google-generativeai = "^0.3.0"
openai = "^1.0.0"
faiss-cpu = "^1.7.4"
scikit-learn = "^1.3.0"
PyPDF2 = "^3.0.1"
python-docx = "^0.8.11"
Pillow = "^10.0.0"
dnspython = "^2.4.0"
python-dateutil = "^2.8.0"
pytz = "^2023.3"

[build-system]
requires = ["poetry-core"]
build-backend = "poetry.core.masonry.api"
